.page-header
	font-size 35px
	text-align center
	padding 0
	border 0
	margin 0
	h2
		font-weight 100
.no-gutter
	padding 0
.container--parallax
	position fixed
	top 0
	z-index 0
.clear
	clear both
.uppercase
	text-transform uppercase
.relative
	position relative
.absolute
	position absolute
.fixed
	position fixed
.float
	&--left
		float left
	&--right
		float right
.inline-block
	display inline-block
	*display inline
	zoom 1
.bx-wrapper
	margin 50px auto 15px auto !important
	text-align center
	img
		display inline-block
.bx-viewport
	background-color transparent !important
	border 0 !important
	-webkit-box-shadow none !important
	box-shadow none !important
	text-align center
	width 90% !important
	margin 0 auto
.stroke-sections
	float left
	width 100%
	img
		width 100%
		display block
		position relative
		z-index 1
		margin 0
		padding 0
.gutter
	padding 0 15px
	&--full
		padding 15px
		
.owl-carousel 
	padding-bottom 70px
.owl-carousel .owl-item
	text-align center
	opacity .5
.owl-carousel .owl-item img
	width auto
	max-width 100%
	margin 0 auto
	-webkit-filter grayscale(1)
	filter grayscale(1)
.owl-carousel .owl-item img:hover
	-webkit-filter grayscale(0)
	filter grayscale(0)
.owl-nav
	display table
	vertical-align middle
	width 100%
	height 200px
	opacity .3
	font-size 24px
	color #5d8531
	text-align center
	&:hover
		color #5d8531
		opacity 1
	span
		display table-cell
		vertical-align middle